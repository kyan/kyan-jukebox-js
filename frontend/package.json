{
  "homepage": "https://kyan.github.io",
  "name": "@jukebox/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "yarn format:check && yarn type-check && yarn lint",
    "fix": "yarn format && yarn lint:fix",
    "test": "yarn validate && react-scripts test --env=jsdom",
    "test:ci": "yarn validate && react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-app": "^4.0.3",
    "dateformat": "^3.0.3",
    "media-progress-timer": "^3.0.0",
    "rc-progress": "^3.1.4",
    "rc-slider": "^9.7.4",
    "react": "^18.0.0",
    "react-audio-player": "^0.11.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^7.2.9",
    "react-scripts": "5.0.1",
    "react-use-googlelogin": "^0.12.5",
    "redux": "^4.2.1",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^2.0.4",
    "socket.io-client": "^4.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.2.3",
    "prettier": "^3.1.1",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.7.0",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.51.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/build/**",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/constants/*.js",
      "!src/**/constants.js",
      "!src/**/constants.ts",
      "!src/components/error-boundary/*.js",
      "!src/search/components/draggable-search-item/*.tsx",
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
