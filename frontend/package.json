{
  "homepage": "https://kyan.github.io",
  "name": "@jukebox/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "build": "bun build ./src/index.tsx --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='REACT_APP_*'",
    "start": "NODE_ENV=production bun src/index.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "bun run format:check && bun run type-check && bun run lint",
    "fix": "bun run format && bun run lint:fix",
    "test": "bun test --bail --preload ./happydom.ts --preload ./src/setupTests-bun.ts",
    "test:ci": "bun test --bail --preload ./happydom.ts --preload ./src/setupTests-bun.ts",
    "test:debug": "bun test --preload ./happydom.ts --preload ./src/setupTests-bun.ts",
    "test:dom": "bun test --preload ./happydom.ts --preload ./src/setupTests-bun.ts"
  },
  "dependencies": {
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.21",
    "media-progress-timer": "^3.0.0",
    "rc-progress": "^3.1.4",
    "rc-slider": "^9.7.4",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^8.0.0",
    "react-refresh": "^0.11.0",
    "redux": "^4.2.1",
    "semantic-ui-react": "^2.1.5",
    "socket.io-client": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@happy-dom/global-registrator": "^19.0.2",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/bun": "^1.2.23",
    "@types/jsdom": "^27.0.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-redux": "^7.1.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-react": "^7.20.5",
    "jsdom": "^27.0.0",
    "prettier": "catalog:",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.93.2",
    "typescript": "^5.9.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
