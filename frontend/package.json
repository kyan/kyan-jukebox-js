{
  "homepage": "https://kyan.github.io",
  "name": "@jukebox/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "bun dev-server.ts",
    "build": "bun build.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "bun run format:check && bun run type-check && bun run lint",
    "fix": "bun run format && bun run lint:fix",
    "test": "bun test.ts",
    "test:ci": "bun test.ts --silent --watchAll=false"
  },
  "dependencies": {
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "classnames": "^2.2.6",
    "dateformat": "^3.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.21",
    "media-progress-timer": "^3.0.0",
    "prop-types": "^15.8.1",
    "rc-progress": "^3.1.4",
    "rc-slider": "^9.7.4",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^8.0.0",
    "react-refresh": "^0.11.0",
    "redux": "^4.2.1",
    "semantic-ui-react": "^2.1.5",
    "socket.io-client": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-redux": "^7.1.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "babel-jest": "^30.2.0",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-react": "^7.20.5",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "jest-transform-stub": "^2.0.0",
    "prettier": "catalog:",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.93.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "tsconfig": {
          "jsx": "react-jsx"
        }
      }
    },
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!<rootDir>/build/**",
      "!src/index.tsx",
      "!src/index-bun.tsx",
      "!src/react-app-env.d.ts",
      "!src/constants/*.js",
      "!src/**/constants.js",
      "!src/**/constants.ts",
      "!src/components/error-boundary/*.js",
      "!src/search/components/draggable-search-item/*.tsx",
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^actions$": "<rootDir>/src/actions",
      "^dashboard$": "<rootDir>/src/dashboard",
      "^search$": "<rootDir>/src/search",
      "^reducers$": "<rootDir>/src/reducers",
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^constants/(.*)$": "<rootDir>/src/constants/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^search/(.*)$": "<rootDir>/src/search/$1",
      "^votes/(.*)$": "<rootDir>/src/votes/$1",
      "^containers/(.*)$": "<rootDir>/src/containers/$1",
      "^hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^middleware/(.*)$": "<rootDir>/src/middleware/$1",
      "^reducers/(.*)$": "<rootDir>/src/reducers/$1",
      "^__mockData__/(.*)$": "<rootDir>/src/__mockData__/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(.*\\.mjs$))"
    ],
    "resetMocks": true,
    "restoreMocks": true,
    "clearMocks": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
