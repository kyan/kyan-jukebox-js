{
  "homepage": "https://kyan.github.io",
  "name": "@jukebox/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "bun dev-server.ts",
    "start:webpack": "node scripts/start.js",
    "build": "bun build.ts",
    "build:webpack": "node scripts/build.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "bun run format:check && bun run type-check && bun run lint",
    "fix": "bun run format && bun run lint:fix",
    "test": "bun run validate && node scripts/test.js --forceExit --detectOpenHandles --maxWorkers=1 --watchAll=false",
    "test:ci": "bun run validate && node scripts/test.js --forceExit --detectOpenHandles --maxWorkers=1 --silent --watchAll=false"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.2.6",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dateformat": "^3.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.21",
    "media-progress-timer": "^3.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "rc-progress": "^3.1.4",
    "rc-slider": "^9.7.4",
    "react": "^18.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^8.0.0",
    "react-refresh": "^0.11.0",
    "redux": "^4.2.1",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
    "semantic-ui-react": "^2.1.5",
    "semver": "^7.3.5",
    "socket.io-client": "^4.2.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.1.1",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.93.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!<rootDir>/build/**",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/constants/*.js",
      "!src/**/constants.js",
      "!src/**/constants.ts",
      "!src/components/error-boundary/*.js",
      "!src/search/components/draggable-search-item/*.tsx",
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "<rootDir>/config/jest/jsdomEnvironment.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "restoreMocks": true,
    "clearMocks": true,
    "forceExit": true,
    "detectOpenHandles": true,
    "maxWorkers": 1
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
