{
  "name": "@jukebox/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node-dev -r dotenv/config --respawn --transpile-only src/index.ts",
    "start:prod": "node dist/index.js",
    "fix": "eslint . --ext .js,.ts --quiet --fix",
    "test": "yarn run lint && yarn run jest",
    "test:ci": "yarn run lint && yarn run jest --coverage",
    "jest": "jest --forceExit --detectOpenHandles --verbose",
    "lint": "tsc --noEmit && eslint . --ext .ts"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^10.0.0",
    "google-auth-library": "^5.3.0",
    "invariant": "^2.2.4",
    "javascript-time-ago": "2.0.13",
    "lodash": "^4.17.15",
    "mongoose": "^6.0.0",
    "mopidy": "^1.3.0",
    "node-cron": "^2.0.3",
    "socket.io": "^4.0.0",
    "spotify-web-api-node": "^4.0.0",
    "tslog": "^3.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.33",
    "@types/javascript-time-ago": "2.0.1",
    "@types/jest": "^26.0.8",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.13",
    "@types/node-cron": "^2.0.3",
    "@types/spotify-web-api-node": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "mockingoose": "2.13.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.0.3"
  }
}
